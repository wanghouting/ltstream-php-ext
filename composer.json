{
    "name": "wanghouting/ltstream-php-ext",
    "description": "ltstream php extension for laravel ",
    "type": "tools",
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "php": "^7.1.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "wanghouting",
            "email": "wanghouting@163.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LTStream\\Extension\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LTStream\\Extension\\LaravelServiceProvider"
            ],
            "aliases": {
                "LTStream": "LTStream\\Extension\\Facades\\LTStream"
            }
        }
    }
}

